#APP
APP_NAME="APP"#change
APP_ENV=production
APP_KEY=#base64:OWzFgOdkjbTIjlvqKgIuBjronMI2ostLN4ilPYSTsWg=
APP_DEBUG=false
APP_URL=#change
APP_PORT=#10003#change
# DO NOT CHANGE THIS
APP_SERVICE=app

# SET THIS FOR DOCKER CONTAINER NAME
CONTAINER_NAME=app#change
# SET THIS FOR WEBSOCKETS CONNECTIONS FROM THE CLIENT
APP_DOMAIN=localhost
# SET THIS FOR WEBSOCKETS CONNECTIONS FROM THE SERVER
WEBSOCKET_DOMAIN=soketi

# WHERE TO MOUNT THE STORAGE
STORAGE_CONTAINER_MOUNT_PATH="/var/www/laravel-dockerized/storage/XXX"#change
STORAGE_DB_MOUNT_PATH="/var/www/laravel-dockerized/storage/XXX"#change

# LOGS
LOG_CHANNEL=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

# DATABASE
DB_CONNECTION=mysql
DB_HOST=mariadb
DB_PORT=3306
DB_DATABASE=app_name#Change
DB_USERNAME=sail
DB_PASSWORD=password

CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# SCOUT
SCOUT_DRIVER=meilisearch
MEILISEARCH_HOST="meilisearch:7700"
MEILISEARCH_KEY=

# This cannot be localhost since connections are made from
# the client browser so we need to connect to the backend ip
PUSHER_HOST="${WEBSOCKET_DOMAIN}"
PUSHER_PORT=6001
PUSHER_SCHEME=http
PUSHER_APP_ID=#app-id
PUSHER_APP_KEY=#app-key
PUSHER_APP_SECRET=#app-secret
BROADCAST_DRIVER=pusher

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${APP_DOMAIN}"
VITE_PUSHER_PORT="6001"
